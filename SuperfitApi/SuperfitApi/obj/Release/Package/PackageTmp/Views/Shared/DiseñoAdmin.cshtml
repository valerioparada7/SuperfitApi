@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/datatablescss")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/fontawesome")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.css">

    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script src="~/scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert-dev.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.css">
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <link rel="shortcut icon" type="image/png" href="https://www.bsite.net/valerioparada/Imagenes/superfiticono.jpeg" />

    <style>
        .color {
            background-color: #007bff;
        }

        .letracolor {
            color: #007bff;
        }

        .colorred {
            background-color: #ff0018;
        }

        .contenido {
            margin-left: 50px;
            margin-right: 50px;
            margin-top: 50px;
            margin-bottom: 50px;
        }
    </style>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg" style="background-color:#007bff;">
            <a class="navbar-brand" href="#"></a>
            <button class="navbar-toggler btn btn-outline-light" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"><i class="fa fa-navicon"></i></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdministracionWebHome", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Home</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("Entrenamientoenvivo", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Entrenamiento en vivo</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminClientesWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Clientes</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminTiporutinaWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Tipo rutina</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminTipoentrenamientoWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Tipo entrenamiento</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminRutinasWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Rutinas</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminEjerciciosWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Ejercicios</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("AdminEstatusWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Estatus</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="navbar-brand" href="@Url.Action("OutWeb", "AdministracionWebHome", null, null)">
                            <span class="menu-collapsed text-white">Salir</span>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <main>
        <div class="contenido">
            @RenderBody()            
        </div>
    </main>
    <footer>
        <hr />
            <p>&copy; @DateTime.Now.Year - Superfly</p>
    </footer>
    @Scripts.Render("~/bundles/datatablesjs")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            validacion();
        });
    function validacion() {
        var direccion = '@Url.Action("Validation", "LoginWeb")';
            $.ajax({
                type: "GET",
                url: direccion,
                success: function (data) {
                    if (data == "True") {
                        window.location.href='@Url.Action("LoginWeb", "LoginWeb")';
                    }
                },
                error: function (xhr, status, error) {
                    console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText)
                    alert("Ocurrio un error verifique su conexion");
                }
            });
        }
    </script>
</body>
</html>
