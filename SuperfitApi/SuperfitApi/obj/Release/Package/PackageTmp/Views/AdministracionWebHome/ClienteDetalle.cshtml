    function GuardarRutina() { 
        swal({
            icon: "https://www.bsite.net/valerioparada/Imagenes/loading.gif"
        });
        var detallerutinaModels = [];
        var l = 0;
        for (var j = 1; j <= 6; j++) {
            l += document.getElementById('ListaEjercicios' + j).getElementsByTagName('tr');
        }

        var c = l.length - 2;       
        if (c > 0) {
            for (var i = 1; i <= 6; i++) {
                var row = document.getElementById('ListaEjercicios' + i).getElementsByTagName('tr');               
                var s = row.length-1;
                for (var y = 1; y <= s; y++) {
                    var model = {};
                    var Dias = {};
                    var Ejercicios = {};
                    var Mensualidad = {};
                    var Rutinas = {};
                    Dias.Id_dia = document.getElementById("iddia" + i + y).innerHTML;
                    Ejercicios.Id_ejercicio = document.getElementById('idejercicio' + i + y).innerHTML;
                    Mensualidad.Id_mensualidad = document.getElementById('idclienteMensualidadDetalle').value;
                    Rutinas.Id_rutina = document.getElementById('idrutina' + i + y).innerHTML;
                    model.Dias = Dias;
                    model.Ejercicios = Ejercicios;
                    model.Mensualidad = Mensualidad;
                    model.Rutinas = Rutinas;
                    model.Repeticiones = document.getElementById('idrepeticion' + i + y).innerHTML;
                    model.Series = document.getElementById('idserie' + i + y).innerHTML;
                    model.Tipo_set = document.getElementById('idset' + i + y).innerHTML;
                    detallerutinaModels.push(model);
                }
            }

            if (detallerutinaModels.length > 0) {
                $.ajax({
                    type: "POST",
                    url: urldetallRutina,
                    data: JSON.stringify(detallerutinaModels),
                    contentType: "application/json",
                    success: function (data) {
                        if (data == "True") {
                            swal({
                                title: "Registro exitoso!",
                                text: "Se guardaron tus datos correctamente",
                                icon: "success"
                            }).then(function () {
                                location.reload();
                            });
                        }
                        else {
                            swal({
                                title: "Warning!",
                                text: "Ocurrio un error",
                                icon: "warning"
                            });
                        }
                    },
                    error: function (xhr, status, error) {
                        console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText)
                        alert("Ocurrio un error verifique su conexion");
                    }
                });
            }
            else {
                swal({
                    title: "Warning",
                    text: "No se detectaron ejercicios por agregar",
                    icon: "warning"
                });
            }
        }
    }

