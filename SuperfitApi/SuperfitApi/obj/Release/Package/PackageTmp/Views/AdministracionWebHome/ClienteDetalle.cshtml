        document.getElementById("idclienteMensualidad").value = Id;
        $.ajax({
            type: "GET",
            url: urlvermensualidad,
            data: { "Id_cliente": Id },
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    var sum = i + 1;
                    var Tiporutina = data[i].Tiporutina;
                    var Mes = data[i].Mes;
                    var Estatus = data[i].Estatus;
                    var pago = data[i].PagoMes;
                    var ima = pago.Ubicacion_imagen_pago;
                    ima = "'" + ima + "'";
                    var html = '';
                    html += '<tr id="fila' + i + '">';
                    html += '<td>' + sum + '</td>';
                    html += '<td><button type="button" class="btn btn-success" onclick = "VerMensualidad(' + data[i].Id_mensualidad +',' + Estatus.Id_estatus +')"> Ver</button></td>';
                    html += '<td><button type="button" class="btn btn-success" onclick="MensualidadDetalle(' + data[i].Id_mensualidad +',' + Estatus.Id_estatus + ')">Rutina</button></td>';
                    html += '<td><button type="button" class="btn btn-success" onclick="VerFotosMedidas(' + data[i].Id_mensualidad + ',' + Estatus.Id_estatus + ')">Medidas</button></td>';
                    html += '<td>' + Tiporutina.Tipo + '</td>';
                    html += '<td>'+Mes.Mes+'</td>';
                    html += '<td>' + Estatus.Descripcion + '</td>';
                    html += '<td><img onclick="Verfotopago(' + ima + ')" src="https://www.bsite.net/valerioparada/' + pago.Ubicacion_imagen_pago + '" width="50" height="50" /> ';
                    html += '<td>' + data[i].Fechainicio+'</td>';
                    html += '<td>' + data[i].Fechafin + '</td>';
                    html += '</tr>';
                    $('#tabladetallemensualidad tbody').append(html);
                    if (Estatus.Id_estatus != 3) {
                        count++;
                    }
