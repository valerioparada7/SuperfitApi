    function AgregarRutina() {
        var ser = $('#Series').val();
        var rep = $('#Repeticiones').val();
        var set = $('#Set').val();
        var rutina = document.getElementById("RutinasList");
        var dia = document.getElementById("Dias");
        var ejercicio = document.getElementById("Ejercicios");
        if (ser == "" && rep == "" && set == "") {
            document.getElementById("Set").style.borderColor = "red";
            document.getElementById("Repeticiones").style.borderColor = "red";
            document.getElementById("Series").style.borderColor = "red";
        }

        if ($('#RutinasList').val() != null)
        {
            if ($('#Ejercicios').val() != "")
            {
                if ($('#Dias').val() != null)
                {
                    if (rep != "" && rep > 0)
                    {
                        if (ser != "" && ser > 0)
                        {
                            if (set != ""&& set > 0)
                            {
                                var tabladia = $('#Dias').val();
                                tabladia -= 1;
                                var i = document.getElementById('ListaEjercicios' + tabladia).getElementsByTagName('tr');
                                var c = i.length;
                                var srcimagen = document.getElementById("previewdeejercicio").src;
                                var html = '';
                                html += '<tr id="fila' + tabladia + c + '">';
                                html += '<td><label id="idmensualidad" style="display:none">' + $('#idclienteMensualidadDetalle').val() + '</label>' + c + '</td> ';
                                html += '<td> <label id="iddia' + tabladia + + c + '" style="display:none">' + $('#Dias').val() + '</label>' + dia.options[dia.selectedIndex].text + '</td> ';
                                html += '<td> <label id="idrutina' + tabladia + + c + '" style="display:none">' + $('#RutinasList').val() + '</label>' + rutina.options[rutina.selectedIndex].text + '</td> ';
                                html += '<td> <label id="idejercicio' + tabladia + + c + '" style="display:none">' + $('#Ejercicios').val() + '</label>' + ejercicio.options[ejercicio.selectedIndex].text;
                                html += '<img src="' + srcimagen +'" id="img' + tabladia + c + '" style="display:inline-block;border-radius:20px;width:5em;"/></td>';
                                html += '<td> <label id="idserie' + tabladia + + c + '">' + $('#Series').val() + '</label></td> ';
                                html += '<td> <label id="idrepeticion' + tabladia + + c + '">' + $('#Repeticiones').val() + '</label></td> ';
                                html += '<td> <label id="idset' + tabladia + + c + '">' + $('#Set').val() + '</label></td> ';
                                html += '<td><button type="button" class="colorred btn btn-danger fa fa-trash" onclick="EliminaFila(' + tabladia + c + ')"></button></td>';
                                html += '</tr>';
                                $('#ListaEjercicios' + tabladia + ' tbody').append(html);
                                document.getElementById("Set").value = "";
                                document.getElementById("Repeticiones").value = "";
                                document.getElementById("Series").value = "";
                            }
                            else {
                                document.getElementById("Set").style.borderColor = "red";
                            }
                        }
                        else {
                            document.getElementById("Series").style.borderColor = "red";
                        }
                    }
                    else {
                        document.getElementById("Repeticiones").style.borderColor = "red";
                    }
                }
                else {
                    document.getElementById("Dias").style.borderColor = "red";
                }
            }
            else {
                document.getElementById("Ejercicios").style.borderColor = "red";
            }
        }
        else {
            document.getElementById("RutinasList").style.borderColor = "red";
        }
    }

