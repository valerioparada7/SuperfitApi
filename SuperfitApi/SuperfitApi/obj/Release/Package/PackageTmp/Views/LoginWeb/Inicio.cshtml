        /*# es para ids*/
        .textos-header {
            animation-name: slideanimation;
            animation-duration: 0.5s;
            position: relative;
            animation-direction: reverse;
            animation-timing-function: linear;
            margin-top: 10%;
            margin-left: 10%;
        }

        @@keyframes slideanimation {
            0% {
                margin-top: 10%;
                margin-left: 10%;
            }

            100% {
                margin-top: 10%;
                margin-left: 0%;
            }
        }
