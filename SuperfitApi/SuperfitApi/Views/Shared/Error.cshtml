<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
    <link rel="shortcut icon" type="image/png" href="https://www.bsite.net/valerioparada/Imagenes/superfiticono.jpeg" />
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>@ViewBag.error</h2>
    </hgroup>
    @if (ViewBag.error != null)
    {        
        <div class="alert alert-danger">
            @ViewBag.error
        </div>
    }
</body>
</html>
